<div class="conatiner rounded-3 shadow-sm p-4 m-5 mt-5 bg-dark">
    <table class="table table-dark table-hover text-center table align-middle ">
        <thead>
            <tr class="fs-5">
                <th scope="col-1">ID</th>
                <th scope="col">Name</th>
                <th scope="col">MFD/PKD</th>
                <th scope="col">Availbility</th>
                <th scope="col">Quantity</th>
                <th scope="col">Price ( Per Item )</th>
            </tr>
        </thead>
        <tbody>
            {{#each rows}}
            <tr class="fs-5">
                <th scope="row" class="py-4 ">{{this.ID}}</th>
                <td class="p_name">{{this.Name}}</td>
                <td id="dat"> {{this.PKD}} </td>
                <td id="avail" class="instock"> {{this.Availbility}}</td>
                <td id='quantity'>{{this.Quantity}}</td>
                <td style="letter-spacing: 1px;">{{this.Price}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
