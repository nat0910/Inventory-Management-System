    <table class="table table-dark table-hover text-center table align-middle ">
      <thead>
        <tr>
          <th scope="col-1">ID</th>
          <th scope="col">Name</th>
          <th scope="col">Type of Commodity</th>
          <th scope="col">Serial No.</th>
          <th scope="col" >MFD/PKD</th>
          <th scope="col" >Expiry Date</th>
          <th scope="col">Quantity</th>
          <th scope="col">Price ( Per Item )</th>
          <th scope="col" class="text-end" style="width: 20%;">Action</th>
        </tr>
      </thead>
      <tbody>
        {{#each rows}}
          <tr>
            <th scope="row" class="py-4">{{this.ID}}</th>
            <td class="p_name">{{this.Name}}</td>
            <td> {{this.Type}} </td>
            <td id='serial'> {{this.Serial}} </td>
            <td id="dat"> {{this.PKD}} </td>
            <td id="dat"> {{this.Expiry}} </td>
            <td>{{this.Quantity}}</td>
            <td style="letter-spacing: 1px;">{{this.Price}}</td>
            <td class="text-end">
              <a type="button" class="btn btn-outline-primary mx-3 px-4" aria-disabled="true"><i class="bi bi-pencil"></i> Edit </a>
              <a href="/{{this.ID}}"class="btn btn-outline-danger  px-4"><i class="bi bi-trash"></i> Delete</a>
            </td>
          </tr>
    {{/each}}
  </tbody>
</table>